<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net46</TargetFramework>
    <AssemblyTitle>VSCMount</AssemblyTitle>
    <Company>VSCMount</Company>
    <Product>VSCMount</Product>
    <Description>Mount VSCs easily</Description>
    <Copyright>Copyright ©  2020</Copyright>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>Window.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Management" Version="4.*" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.*" />
    <PackageReference Include="Microsoft.CSharp" Version="4.*" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AlphaFS" Version="2.2.6" />
    <PackageReference Include="Costura.Fody" Version="5.1.0">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Exceptionless" Version="4.6.2" />
    <PackageReference Include="FluentCommandLineParser" Version="1.4.3" />
    <PackageReference Include="NLog" Version="4.7.8" />
    <PackageReference Include="ServiceStack.Text" Version="5.10.4" />
    <PackageReference Include="WmiLight" Version="2.1.17172.10" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Changes.txt" />
    <Content Include="Window.ico" />
  </ItemGroup>
</Project>